"  -h, --help                 print this help\n"
"  -i, --ignore-case          fold character case while comparing\n"
"  -l, --less-mode            variation of printer mode for \"less\"\n"
"  -n, --avoid-wraps          do not extend fields through newlines\n"
"  -p, --printer              overstrike as for printers\n"
msgstr ""
"  -a, --auto-pager\t\ttự động gọi trình dàn trang\n"
"  -h, --help                 \thiển thị trợ giúp này\n"
"  -i, --ignore-case\t\tkhông phân biệt chữ hoa/thường khi so sánh\n"
"  -l, --less-mode        \tbiến thể chế độ máy in cho \"less\"\n"
"  -n, --avoid-wraps  \tkhông mở rộng trường qua ký tự dòng mới\n"
"  -p, --printer              \tghi đè giống như cho máy in\n"

#: src/wdiff.c:1196 src/wdiff2.c:209
#, c-format
msgid ""
"  -s, --statistics           say how many words deleted, inserted etc.\n"
"  -t, --terminal             use termcap as for terminal displays\n"
"  -v, --version              print program version then exit\n"
"  -w, --start-delete=STRING  string to mark beginning of delete region\n"
"  -x, --end-delete=STRING    string to mark end of delete region\n"
"  -y, --start-insert=STRING  string to mark beginning of insert region\n"
"  -z, --end-insert=STRING    string to mark end of insert region\n"
msgstr ""
"  -s, --statistics \tthông báo số các từ bị xoá, đã chèn v.v.\n"
"  -t, --terminal\tdùng termcap giống như cho bộ trình bày thiết bị cuối\n"
"  -v, --version     \thiển thị thông tin về phiên bản chương trình, sau đó thoát\n"
"  -w, --start-delete=CHUỖI\t\tchuỗi sẽ đánh dấu bắt đầu vùng xoá\n"
"  -x, --end-delete=CHUỖI    \tchuỗi sẽ đánh dấu kết thúc vùng xoá\n"
"  -y, --start-insert=CHUỖI  \t\tchuỗi sẽ đánh dấu bắt đầu vùng chèn\n"
"  -z, --end-insert=CHUỖI    \t\tchuỗi sẽ đánh dấu kết thúc vùng chèn\n"

