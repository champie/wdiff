#: src/mdiff.c:1276
#, c-format
msgid "Read summary: %d files, %d items\n"
msgstr "Resumo da leitura: %d arquivos, %d itens\n"

#: src/mdiff.c:1671
msgid "Sorting"
msgstr "Ordenando"

#: src/mdiff.c:1688
