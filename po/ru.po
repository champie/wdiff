#: src/mdiff.c:197
#, c-format
msgid "Reading %s ..."
msgstr "Чтение %s ..."

#: src/mdiff.c:230
#, c-format
msgid " %d lines\n"
msgstr " %d строк\n"

#: src/mdiff.c:558 src/unify.c:271 src/wdiff.c:1100
#, c-format
msgid "Try `%s --help' for more information.\n"
msgstr "Попробуйте `%s --help' для получения более подробного описания.\n"

#: src/mdiff.c:563
#, c-format
msgid "\nUsage: %s [OPTION]... FILE...\n"
msgstr "\nИспользование: %s [КЛЮЧ]... ФАЙЛ...\n"

#: src/mdiff.c:566
msgid ""
"\n"
"  -t, --threshold=LINES   do not consider clusters of less than LINES lines\n"
"  -s, --statistics        produce statistics on stdout\n"
"  -v, --verbose           report progress of operations on stderr\n"
"      --help              display this help and exit\n"
"      --version           output version information and exit\n"
msgstr ""
"\n"
"  -t, --threshold=ЧИСЛО   не рассматривать кластеры короче заданного ЧИСЛА строк\n"
"  -s, --statistics        печатать на стандартный вывод статистические сведения\n"
"  -v, --verbose           печатать промежуточные результаты в стандартный поток\n"
"                          ошибок\n"
"      --help              показать эту справку и выйти\n"
"      --version           показать информацию о версии и выйти\n"

#: src/mdiff.c:574
msgid "\nReport bugs to <wdiff-bugs@gnu.ai.mit.edu>.\n"
msgstr "\nОб ошибках сообщайте <wdiff-bugs@gnu.ai.mit.edu>.\n"

