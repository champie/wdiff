msgstr "Неверный заголовок объединенного фрагмента различия в строке %ld"

#: src/unify.c:592
#, c-format
msgid "Malformed unified diff at line %ld"
msgstr "Неправильная объединенная запись различия в строке %ld"

#: src/unify.c:623
#, c-format
msgid "Context diff missing `old' header at line %ld"
msgstr "Пропущен заголовок `старой части' контекстного фрагмента различия в строке %ld"

#: src/unify.c:667 src/unify.c:686 src/unify.c:757 src/unify.c:775
#, c-format
