#: src/wdiff.c:1174
msgid "wdiff - Compares words in two files and report differences.\n"
msgstr "wdiff - Vergelijkt woorden in twee bestanden en rapporteert verschillen.\n"

#: src/wdiff.c:1177 src/wdiff2.c:185
#, c-format
msgid ""
"\n"
"Usage: %s [OPTION]... FILE1 FILE2\n"
msgstr ""
"\n"
"Gebruik:  %s [OPTIE]... BESTAND1 BESTAND2\n"

#: src/wdiff.c:1181 src/wdiff2.c:189
#, c-format
msgid ""
"Mandatory arguments to long options are mandatory for short options too.\n"
"\n"
"  -C, --copyright            print copyright then exit\n"
"  -K, --no-init-term         like -t, but no termcap init/term strings\n"
"  -1, --no-deleted           inhibit output of deleted words\n"
"  -2, --no-inserted          inhibit output of inserted words\n"
"  -3, --no-common            inhibit output of common words\n"
msgstr ""
"(De argumenten bij lange opties gelden ook voor de korte vormen.)\n"
"\n"
"  -C, --copyright            auteursrecht tonen en stoppen\n"
"  -K, --no-init-term         als '-t', maar zonder 'init'- of 'term'-reeksen\n"
"  -1, --no-deleted           verwijderde woorden niet weergeven\n"
"  -2, --no-inserted          ingevoegde woorden niet weergeven\n"
"  -3, --no-common            gemeenschappelijke woorden niet weergeven\n"

#: src/wdiff.c:1189 src/wdiff2.c:198
#, c-format
msgid ""
"  -a, --auto-pager           automatically calls a pager\n"
"  -h, --help                 print this help\n"
"  -i, --ignore-case          fold character case while comparing\n"
"  -l, --less-mode            variation of printer mode for \"less\"\n"
"  -n, --avoid-wraps          do not extend fields through newlines\n"
"  -p, --printer              overstrike as for printers\n"
msgstr ""
"  -a, --auto-pager           automatisch een 'pager' aanroepen\n"
"  -h, --help                 deze hulptekst tonen en stoppen\n"
"  -i, --ignore-case          verschil tussen hoofd- en kleine letters negeren\n"
"  -l, --less-mode            uitvoermodus voor 'less'\n"
"  -n, --avoid-wraps          velden niet voortzetten op een volgende regel\n"
"  -p, --printer              'overstrike' gebruiken als bij printers\n"

#: src/wdiff.c:1196 src/wdiff2.c:209
#, c-format
msgid ""
"  -s, --statistics           say how many words deleted, inserted etc.\n"
"  -t, --terminal             use termcap as for terminal displays\n"
"  -v, --version              print program version then exit\n"
"  -w, --start-delete=STRING  string to mark beginning of delete region\n"
"  -x, --end-delete=STRING    string to mark end of delete region\n"
"  -y, --start-insert=STRING  string to mark beginning of insert region\n"
"  -z, --end-insert=STRING    string to mark end of insert region\n"
msgstr ""
"  -s, --statistics           aantal verwijderingen, invoegingen, enz. tonen\n"
"  -t, --terminal             'termcap' gebruiken als voor een terminal\n"
"  -w, --start-delete=STRING  markering voor begin van verwijderd stuk\n"
"  -x, --end-delete=STRING    markering voor einde van verwijderd stuk\n"
"  -y, --start-insert=STRING  markering voor begin van ingevoegd stuk\n"
"  -z, --end-insert=STRING    markering voor einde van ingevoegd stuk\n"

#: src/wdiff.c:1319
msgid "Cannot use -t, termcap not available."
msgstr "Kan optie '-t' niet gebruiken; termcap is niet beschikbaar."

